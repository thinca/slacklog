tus) abort\n\t\" TODO create response object\n\tcall call(a:cb, [{'name':'gorilla'}])\nendfunction\n\nfunction! s:gorilla_cb(response) abort\n\techo a:response\nendfunction\n\nlet s:header_file = tr(tempname(), '\\', '/')\nlet s:response_file = tr(tempname(), '\\', '/')\nlet s:image_name = 'hello-world:latest'\n\nlet cmd = 'curl --unix-socket /var/run/docker.sock -X POST --dump-header ' .. s:header_file .. ' --output ' .. s:response_file .. ' <http://localhost/images/create?fromImage=>' .. s:image_name\n\ncall job_start(cmd, {\n\t\t\t\\ 'exit_cb': function('s:exit_cb', [s:header_file, s:response_file, function('s:gorilla_cb')])\n\t\t\t\\ })\n```","ts":"1563120143.065900","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"4027481f-5307-4233-b0d4-5bf8cdd2b2df","type":"message","user":"U03C71D7H","text":"特に問題なさそう。逆に気になってる部分とかありますか?","ts":"1563120899.066300","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"e7434dff-62ed-4c9c-8ae3-0d94f2e0642b","type":"message","user":"UDDCM376D","text":"いえ、もっと簡単なやり方無いかなって思ってきいてみました","ts":"1563120972.066700","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"e1ccd969-eea5-4bb9-aa47-7d2c21e1e5ec","type":"message","user":"UDDCM376D","text":"問題なさそうなら、これで実装してみようと思います","ts":"1563121001.067100","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"e99c973c-f510-4273-b7b8-d07471520b82","type":"message","user":"UDDCM376D","text":"[質問]\n```\ncurl -s --unix-socket /var/run/docker.sock -X POST --dump-header '/var/folders/yy/krfkwtjx48xb0m25vb5ncth40000gn/T/vc88d8o/81' --output '/var/folders/yy/krfkwtjx48xb0m25vb5ncth40000gn/T/vc88d8o/82' 'http:\n//localhost/images/create?fromImage=hello-world:latest'\n```\nを `job_start` で実行するとcurlの23エラー（ファイル書き込みエラー）になるのですが、どこか変ですか？","ts":"1563125051.068100","edited":{"user":"UDDCM376D","ts":"1563125074.000000"},"team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"7528b4c8-c403-4e61-8b81-e137c6514edc","type":"message","user":"UDDCM376D","text":"ちなみに↑はターミナルでそのまま実行するとうまくいきます","ts":"1563125106.068900","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":null,"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"type":"message","user":"UDDCM376D","text":"スースはこんな感じになっています","ts":"1563126668.069100","files":[{"id":"FL7P2LXRP","created":1563126668,"timestamp":1563126668,"name":"-.pl","title":"無題","mimetype":"text/plain","image_exif_rotation":0,"filetype":"perl","pretty_type":"Perl","user":"UDDCM376D","mode":"snippet","editable":true,"is_external":false,"external_type":"","size":1109,"url":"","url_download":"","url_private":"https://files.slack.com/files-pri/T03C4RC8V-FL7P2LXRP/-.pl","url_private_download":"https://files.slack.com/files-pri/T03C4RC8V-FL7P2LXRP/download/-.pl","original_h":0,"original_w":0,"thumb_64":"","thumb_80":"","thumb_160":"","thumb_360":"","thumb_360_gif":"","thumb_360_w":0,"thumb_360_h":0,"thumb_480":"","thumb_480_w":0,"thumb_480_h":0,"thumb_720":"","thumb_720_w":0,"thumb_720_h":0,"thumb_960":"","thumb_960_w":0,"thumb_960_h":0,"thumb_1024":"","thumb_1024_w":0,"thumb_1024_h":0,"permalink":"https://vim-jp.slack.com/files/UDDCM376D/FL7P2LXRP/-.pl","permalink_public":"https://slack-files.com/T03C4RC8V-FL7P2LXRP-0885bf8864","edit_link":"https://vim-jp.slack.com/files/UDDCM376D/FL7P2LXRP/-.pl/edit","preview":"","preview_highlight":"","lines":0,"lines_more":0,"is_public":true,"public_url_shared":false,"channels":null,"groups":null,"ims":null,"initial_comment":{},"comments_count":0,"num_stars":0,"is_starred":false,"shares":{"public":null,"private":null},"displa