te! ' . line\n    endif\n  endfor\nendfunction\n\nlet g:fzf_preview_custom_processes['open-file'] = fzf_preview#remote#process#get_default_processes('open-file', 'coc')\nlet g:fzf_preview_custom_processes['open-file']['ctrl-s'] = g:fzf_preview_custom_processes['open-file']['ctrl-x']\ncall remove(g:fzf_preview_custom_processes['open-file'], 'ctrl-x')\n\nlet g:fzf_preview_buffer_delete_processes = copy(g:fzf_preview_custom_processes['open-file'])\nlet g:fzf_preview_buffer_delete_processes['ctrl-x'] = get(function('s:buffers_delete_from_lines'), 'name')\n\nnnoremap &lt;silent&gt; [fzf-p]b :&lt;C-u&gt;FzfPreviewBuffers --processes=fzf_preview_buffer_delete_processes&lt;CR&gt;```\n多分やるならこんな感じ","ts":"1594872830.311900","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"xdm"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"d184ef9c-d5cd-4233-917f-82a0374aec08","type":"message","user":"U8996GYBF","text":"バッファ一覧については `&lt;C-q&gt;` あたりのプラグイン側デフォ挙動をQuickFixへの出力からバッファの削除に上書きしちゃおうかなぁ","ts":"1594872916.313000","edited":{"user":"U8996GYBF","ts":"1594873098.000000"},"team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"EBjvE"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"bd538a96-ed19-40c1-8e60-ebc3cd232fb7","type":"message","user":"U8996GYBF","text":"C-xがデフォsplitだから（本当はこれを削除に当てたい）、C-qかC-cあたりをバッファの削除に当てるのが妥当な感じがしている","ts":"1594872984.313600","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"HJpq"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"5e595781-3f1f-4b2a-9a4b-62d2c6b3a3f7","type":"message","user":"U8996GYBF","text":"この辺を便利にする感じにやっていかないとtelescope.nvimが上位互換になってしまう可能性の高さを感じている","ts":"1594873189.314900","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"TuQ9N"}],"source_team":"T0