[{"client_msg_id":"3983DC72-387D-464E-A5DB-F3EB03129455","type":"message","user":"UDDCM376D","text":"<https://zenodo.org/record/3710954|https://zenodo.org/record/3710954>","ts":"1588811306.478300","attachments":[{"fallback":"Zenodo: JavaScript: The First 20 Years","id":1,"title":"JavaScript: The First 20 Years","title_link":"https://zenodo.org/record/3710954","text":"How a sidekick scripting language for Java, created at Netscape in a ten-day hack, ships first as a de facto Web standard and eventually becomes the world’s most widely used programming language. This paper tells the story of the creation, design, evolution, and standardization of the JavaScript language over the period of 1995–2015. But the story is not only about the technical details of the language. It is also the story of how people and organizations competed and collaborated to shape the JavaScript language which dominates the Web of 2020.","blocks":null,"service_name":"Zenodo","from_url":"https://zenodo.org/record/3710954","original_url":"https://zenodo.org/record/3710954"}],"team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"=DWl"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"48BAED90-3131-43F4-9858-28194CE03DAB","type":"message","user":"UDDCM376D","text":"jsの20年間の歴史","ts":"1588811314.478700","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"4Pe"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"FC5E6CAC-6335-4A19-A6AB-62D26F7FD413","type":"message","user":"UDDCM376D","text":"とても興味ある","ts":"1588811319.478900","team":"T03C4RC8V","replace_original":false,"delete_original":false,"blocks":[{"type":"rich_text","block_id":"gIDA"}],"source_team":"T03C4RC8V","user_team":"T03C4RC8V"},{"client_msg_id":"0BB86F03-7120-4DEA-85AC-7B75ED16FFB5","type":"message","user":"UDDCM376D","text":"まだ読んで無い","ts":"1588811321.479100","team":"T03C4RC8V","replace_