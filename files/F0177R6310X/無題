type GitBranchActionData = {
  command: FzfCommandName
  type: "git-branch-actions"
  action: "diff" | "yank"
  branches: Array<string>
}

const foo = (data: GitBranchActionData | FooData) => {
  if (data.type !== "git-branch-actions") {
    throw new Error()
  }

  switch (data.action) {
    case "diff": {
      break
    }
    case "yank": {
      break
    }

    default: {
      // OK
      const _: never = data.action
      // NG
      const _: never = data
    }
  }
}